 Host *
     ForwardAgent no
     ForwardX11 no
     ForwardX11Trusted yes
     Port 22
     Protocol 2
     ServerAliveInterval 60
     ServerAliveCountMax 30

Host github.com
    Hostname github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa

Host dandy-dev-unstable
    Hostname ec2-3-83-251-178.compute-1.amazonaws.com
    User ubuntu
    IdentityFile ~/.ssh/dandy.key

Host dandy-prod-1
    Hostname ec2-18-234-97-47.compute-1.amazonaws.com
    User ec2-user
    IdentityFile ~/.ssh/dandy.key
